<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kakaoMapper">

	
	<resultMap type="com.study.test.kakao.vo.KakaoVO" id="kakao">
		<id column="ACCOUNT_EMAIL" property="accountEmail"/>
		<result column ="PROFILE_NICKNAME" property="profileNickname"/>
		<result column="GENDER" property="gender"/>
	</resultMap>
	

	<insert id="kakaoLogin">
		INSERT INTO KAKAO_LOGIN(
			PROFILE_NICKNAME
			, ACCOUNT_EMAIL
			, GENDER			
		) VALUES (
			#{profileNickname}
			, #{accountEmail}
			, #{gender}
		)
	</insert>


</mapper>


























